<style>

	.hovered:hover {
		cursor: pointer;
	}

	.outLogin {
		opacity: 1;
		transition: opacity 0.2s ease-out, transform 0.6s;
	}

	.outRegister {
		opacity: 1;
		transition: opacity 0.2s ease-out, transform 0.6s;
	}

	.inLogin {
		opacity: 1;
		transition: opacity 0.2s ease-out, transform 0.6s;
	}

	.inRegister {
		opacity: 1;
		transition: opacity 0.2s ease-out, transform 0.6s;
	}
</style>

<script>


	function getOutLogin() {
		$("#login_form").removeClass("bounceIn").addClass("hided");
		$("#register_form").removeClass("bounceOut hided").addClass("bounceIn");
		$('#login input').tooltipster('hide');
	}

	function getOutRegister() {

		$("#register_form").removeClass("bounceIn").addClass("hided");
		$("#login_form").removeClass("bounceOut hided").addClass("bounceIn");
		$('#register input').tooltipster('hide');





	}
</script>
<style>
	.hided{
		display: none;
	}
</style>
<script type="text/javascript">



	jQuery(document).ready(function () {
