<!-- ==================================================
Body: Classes
header-shadow - Set shadow for header & menu
dark-menu - Change menu to dark [Don't use with light-header]
light-header - Set light background to header [Don't use with dark-menu]
dark-header - Set dark background to header
bold-menu - For bolder links
semi-trans-header - set transparency to header [Use with 'light-header' or 'dark-header']
center-logo - Logo at center
small-header - For lesser height
nav-hidden - Open nav on icon click
================================================== -->
<body >

<!-- ========== HEADER ========== -->


<!-- ========== MAIN ========== -->
<div class="main" >
	<div class="video-background" style="min-height: 100vh;" data-vide-bg="images/backgrounds/test.mp4" data-vide-options="posterType: jpg, loop: true, muted: true, position: 0% 0%">

			<div class="pt90">
				</div>
				<div class="col-md-3 login-box animated bounceIn" id="login_form">
					<img src="images/logos/W_logo_black.png" alt="Salt" class="login-logo">
					<form action="#" class="" id="login">

						<h3 class="section-title text-center pb20 pt10">Connexion</h3>
						<div class="form-group">
							<label class="label-test" for="identifiant">Adresse mail </label>
							<p class="fa fa-user input-icon"></p>
							<input type="text" class="form-control icon-form" id="login_adresse" name="login_adresse"
								   placeholder="Email">
							<i></i>
						</div>

						<div class="form-group">
							<label class="label-test" for="password">Password</label>
							<p class="fa fa-lock input-icon"></p>
							<input type="password" class="form-control icon-form" id="login_password"
								   name="login_password" placeholder="Password">
							<i></i>
						</div>
						<div class="form-group-lg">
							<div class="checkbox">
								<input id="checkbox-5" class="st-checkbox" name="checkbox-5" type="checkbox" checked="">
								<label for="checkbox-5" class="st-checkbox-2-label">Se rappeller de moi</label>
							</div>
						</div>
						<div class="text-center">
							<button type="submit"
									class="st-btn primary-btn hvr-back hvr-sweep-to-right center-block text-center">
								Connexion
							</button>
						</div>

						<a onclick="getOutLogin()" class="hovered">
							<div class="alert alert-info" role="alert"><strong>Not registred yet ?</strong> Click here
								to register
							</div>
						</a>
					</form>
				</div>

				<div class="col-md-5 login-box animated bounceIn" id="register_form">
					<img src="images/logos/W_logo_black.png" alt="Salt" class="login-logo">
					<form action="#" class="" id="register">
						<h3 class="section-title text-center pb20 pt10">Inscription</h3>
						<div class="row">
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-test" for="identifiant">Lastname</label>
									<p class="fa fa-user input-icon"></p>
									<input type="text" class="form-control icon-form" id="register_lastname"
										   name="register_lastname" placeholder="Lastname">
									<i></i>
								</div>
							</div>
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-test" for="email">Name</label>
									<p class="fa fa-user input-icon"></p>
									<input type="text" class="form-control icon-form" id="register_name"
										   name="register_name" placeholder="Name">
									<i></i>
								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-test" for="email">Password</label>
									<p class="fa fa-lock input-icon"></p>
									<input type="password" class="form-control icon-form" id="register_password"
										   name="register_password" placeholder="Type your password">
									<i></i>
								</div>
							</div>
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-test" for="email">Password confirm</label>
									<p class="fa fa-lock input-icon"></p>
									<input type="password" class="form-control icon-form" id="register_password_confirm"
										   name="register_password_confirm" placeholder="Retype your password">
									<i></i>
								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-test" for="email">Mail adress</label>
									<p class="fa fa-envelope input-icon"></p>
									<input type="text" class="form-control icon-form" id="register_address"
										   name="register_address" placeholder="Mail adress">
									<i></i>
								</div>
							</div>
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-test" for="email">Gender</label>
									<p class="fa fa-intersex input-icon"></p>
									<select class="form-control icon-form" id="register_gender" name="register_gender"
											style="font-size: 14px">
										<option value="Male">Male</option>
										<option value="Female">Female</option>
									</select>
									<i></i>
								</div>
							</div>

						</div>
						<div class="form-group-lg">
							<div class="checkbox">
								<input id="cgu" class="st-checkbox" name="cgu" type="checkbox" checked="">
								<label for="cgu" class="st-checkbox-2-label">J'accepte les conditions générales
									d'utiliation</label>
							</div>
						</div>
						<div class="text-center">
							<button type="submit"
									class="st-btn primary-btn hvr-back hvr-sweep-to-right center-block text-center">
								S'inscrire
							</button>
						</div>
						<a onclick="getOutRegister()" class="hovered">
							<div class="alert alert-success" role="alert"><strong>Already registred ?</strong> Click
								here to login
							</div>
						</a>
					</form>
				</div>
			</div>
		</div>




</div> <!-- end row -->
<style>
	.effect7
	{
		-moz-box-shadow: 0px 0px 20px 0px #000000;
